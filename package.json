    "@ianwalter/subpub": "^1.0.0"
  },
  "devDependencies": {
    "@babel/plugin-transform-runtime": "^7.2.0",
    "@babel/preset-env": "^7.2.3",
    "@ianwalter/dist": "^1.4.0",
    "@ianwalter/puppeteer-helper": "^1.0.0",
    "ava": "^1.1.0",
    "common-tags": "^1.8.0",
    "eslint": "^4.19.1",
